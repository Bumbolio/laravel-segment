{
    "name": "octohk/laravel-segment",
    "description": "laravel-segment",
    "keywords": [
        "octohook",
        "laravel-segment"
    ],
    "homepage": "https://github.com/octohk/laravel-segment",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Jones",
            "email": "sam@slashequip.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^8.0",
        "myclabs/php-enum": "^1.8"

    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.13",
        "pestphp/pest": "^1.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Octohook\\LaravelSegment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octohook\\LaravelSegment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octohook\\LaravelSegment\\LaravelSegmentServiceProvider"
            ],
            "aliases": {
                "Segment": "Octohook\\LaravelSegment\\Facades\\Segment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
